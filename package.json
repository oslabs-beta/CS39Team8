{
  "name": "Squrl",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest --verbose",
    "node": "tsc && node --max-old-space-size=8192  dist/backend/server.js",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.config.js --mode development && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js --mode development",
    "react": "cross-env NODE_ENV=production webpack --config webpack.react.config.js --mode production",
    "electron": "cross-env NODE_ENV=production webpack --config webpack.electron.config.js --mode production",
    "build": "npm run react && npm run electron && electron .",
    "electron:run": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a4heng/Squrl.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/a4heng/Squrl/issues"
  },
  "homepage": "https://github.com/a4heng/Squrl#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/file-saver": "^2.0.1",
    "babel-loader": "^8.2.1",
    "css-loader": "^5.0.1",
    "electron": "^11.0.0",
    "electron-builder": "^22.9.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.2.0",
    "electron-react-devtools": "^0.5.3",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "react-devtools-electron": "^4.7.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/d3": "^6.2.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.1.4",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.9",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.7",
    "@types/randomcolor": "^0.5.5",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@visx/brush": "^1.2.0",
    "@visx/gradient": "^1.0.0",
    "@visx/mock-data": "^1.0.0",
    "@visx/pattern": "^1.1.0",
    "@visx/scale": "^1.1.0",
    "axios": "^0.21.0",
    "bytes": "^3.1.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "chartjs-plugin-labels": "^1.1.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "custom-electron-titlebar": "^3.2.5",
    "d3": "^6.3.0",
    "d3-array": "^2.9.0",
    "electron-dl": "^3.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "file-saver": "^2.0.5",
    "file-url": "^3.0.0",
    "http": "0.0.1-security",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "randomcolor": "^0.6.2",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "sweetalert": "^2.1.2"
  },
  "build": {
    "appId": "com.example.SQURL",
    "productName": "SQURL",
    "directories": {
      "output": "build",
      "buildResources": "dist"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "assets/**/*"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "identity": null
    },
    "dmg": {
      "iconSize": 100,
      "window": {
        "width": 544,
        "height": 408
      },
      "contents": [
        {
          "x": 145,
          "y": 257
        },
        {
          "x": 400,
          "y": 255,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/jest.config.js"
    ],
    "moduleNameMapper": {
      ".+\\.(svg|png|jpg)$": "identity-obj-proxy"
    }
  }
}
